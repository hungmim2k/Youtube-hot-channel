{
  "name": "youtube-hot-channels-finder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:css": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
    "build:css": "tailwindcss -i ./src/input.css -o ./public/output.css && tailwindcss -i ./index.css -o ./public/index.css",
    "build": "npm run build:css && vite build",
    "preview": "vite preview",
    "migrate": "ts-node database/migrate.ts",
    "migrate:js": "node database/migrate.js",
    "verify-admin": "ts-node database/verify-admin.ts",
    "verify-tables": "node database/verify-tables.js",
    "test-login": "ts-node database/test-login.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "autoprefixer": "^10.4.17",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "postcss": "^8.4.35",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^14.0.5",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
